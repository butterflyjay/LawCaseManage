/* pages/manger-browse/manger-browse.wxss */
.container {
  width: 100%;
  background-color: white;
}
.inp {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 30px;
  background-color: whitesmoke;
}
.inpFind {
  text-align: center;
  height: 60%;
  width: 90%;
  border-radius: 5px;
  background-color: white;
}
.search {
  height: 22px;
  width: 22px;
  background-color: white;
}
.main {
  width: 100%;
}
.ofmain {
  background-color: #FFFFFF;
}
.table {
  display: flex;
  flex-direction: row;
  margin-left: 20px;
  padding-bottom: 15px;
  margin-top: 15px;
  width: 90%;
  border-bottom: 1px solid rgb(226, 223, 223);
 }
 .tr {
   margin-bottom: 5px;
 }
 .type {
   color: red;
 }
 .main-info {
   font-size: 12px;
 }
 .all-info {
    margin-left: auto;    /*使用flex布局的元素 不能使用float*/
   height: fit-content;
   font-size: 11px;
   color: #5599FF;
 }
 
 

